<div>
    <%= form_with model: shop, data: { turbo: false } do |f| %>

      <div>
        <%= f.label :title %>
        <%= f.text_field :title %>
      </div>
      <div>
        <%= f.label :description %>
        <%= f.text_field :description %>
      </div>
      <div>
        <%= f.label :tel %>
        <%= f.text_field :tel %>
      </div>

      <div><%= f.submit %></div>
    <% end %>
  </div>