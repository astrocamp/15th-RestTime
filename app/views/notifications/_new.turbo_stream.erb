<%= turbo_stream.prepend "notification" do %>
   <div>
      <p><%= notification.title %></p>
      <p><%= notification.message %></p>
   </div>
   <hr>
<% end %>
      

<%= turbo_stream.replace "badge" do %>
   <span id="badge" data-notification-target="element" class="badge badge-xs badge-primary z-[1] indicator-item"></span>
<% end %>


